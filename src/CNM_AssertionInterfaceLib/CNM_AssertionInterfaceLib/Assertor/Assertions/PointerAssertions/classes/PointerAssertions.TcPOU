    <Method Name="isNotOverlapping" Id="{41d2f5c9-34a5-4156-ac03-97141240756a}">
      <Declaration><![CDATA[(*

## Short summary
This assertion method checks if memory areas are overlapping

.. <legal notes>

legal notes
=================
| SPDX-FileCopyrightText: © 2022 ekvip automation GmbH <info@ekvip.de>
| SPDX-License-Identifier: Apache-2.0
| For details check: Apache-2.0_

.. _Apache-2.0: https://www.apache.org/licenses/LICENSE-2.0

.. </legal notes>



*)

METHOD isNotOverlapping
VAR_INPUT
	(* memory address *)
	address1	:PVOID;
	(* memory address *)
	address2	:PVOID;
	(*the size of the memory area identified by address1*)
	sizeInBytes1		:UDINT;
	(*the size of the memory area identified by address2*)
	sizeInBytes2		:UDINT;
	(* message if the assertion is false *)
	message		:AssertMessage;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF 	(( 
		address1 = address2
	) OR_ELSE(
		//address1 + sizeInBytes1 < address1 is to protect overflows caused by the calculation address1 + sizeInBytes1
		(address1 < address2) AND_THEN ((address1 + sizeInBytes1 < address1) OR_ELSE ((address1 + sizeInBytes1) >= address2))
	) OR_ELSE (
		//address2 + sizeInBytes2 < address2 is to protect overflows caused by the calculation address2 + sizeInBytes2
		(address1 > address2) AND_THEN (((address2 + sizeInBytes2 < address2) OR_ELSE ((address2 + sizeInBytes2) >= address1)))
	)
) THEN
		THIS^.assertionWasWrong(message, THIS^.getDebugInfo('isNotOverlapping'));
END_IF

 ]]></ST>
      </Implementation>
    </Method>
