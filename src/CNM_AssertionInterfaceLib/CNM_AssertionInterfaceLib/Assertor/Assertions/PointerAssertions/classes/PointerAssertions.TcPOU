    <Method Name="isAligned" Id="{1049eedc-4d69-47ab-8f5c-cbf4b77c6ab7}">
      <Declaration><![CDATA[(*

## Short summary
This assertion method checks if pointer is aligned with register size.

.. <legal notes>

legal notes
=================
| SPDX-FileCopyrightText: © 2025 ekvip automation GmbH <info@ekvip.de>
| SPDX-License-Identifier: Apache-2.0
| For details check: Apache-2.0_

.. _Apache-2.0: https://www.apache.org/licenses/LICENSE-2.0

.. </legal notes>



*)

METHOD isAligned
VAR_INPUT
	(* memory address *)
	address		:PVOID;
	(* message if the assertion is false *)
	message		:AssertMessage;
END_VAR
VAR CONSTANT
	SIZE :PVOID := SIZEOF(__XWORD);
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF (address MOD SIZE > 0) THEN
	THIS^.assertionWasWrong(message, THIS^.getDebugInfo('isAligned'));
END_IF
]]></ST>
      </Implementation>
    </Method>
