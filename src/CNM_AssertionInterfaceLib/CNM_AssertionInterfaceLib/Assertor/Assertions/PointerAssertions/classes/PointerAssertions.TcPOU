    <Method Name="isOverlapping" Id="{c9eb8344-714a-4db7-9fae-856fe8a9cf66}">
      <Declaration><![CDATA[(*

## Short summary
This assertion method checks if memory areas are overlapping

.. <legal notes>

legal notes
=================
| SPDX-FileCopyrightText: © 2022 ekvip automation GmbH <info@ekvip.de>
| SPDX-License-Identifier: Apache-2.0
| For details check: Apache-2.0_

.. _Apache-2.0: https://www.apache.org/licenses/LICENSE-2.0

.. </legal notes>



*)

METHOD isOverlapping
VAR_INPUT
	(* memory address *)
	address1	:PVOID;
	(* memory address *)
	address2	:PVOID;
	(*the size of the memory area identified by address1*)
	sizeInBytes1		:UDINT;
	(*the size of the memory area identified by address2*)
	sizeInBytes2		:UDINT;
	(* message if the assertion is false *)
	message		:AssertMessage;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[RETURN(address1 = address2);
IF ((
		(address1 < address2) AND_THEN ((address1 + sizeInBytes1) < address2)
	) OR_ELSE (
		(address1 > address2) AND_THEN ((address2 + sizeInBytes2) < address1)
	)
) THEN
	THIS^.assertionWasWrong(message, THIS^.getDebugInfo('isOverlapping'));
END_IF;
	]]></ST>
      </Implementation>
    </Method>
    <Method Name="isValid" Id="{4d85afcc-514f-4d0b-8ec9-b6734eb2854b}">
      <Declaration><![CDATA[(*

## Short summary
This assertion method checks if pointer is valid.
That means the pointer and size are not zero,
the pointer is in a valid area.

**Attention:** 
	Validity of the pointer will be proofed for 32 bit systems only.
	For other systems it's an optimistic estimate.

.. <legal notes>

legal notes
=================
| SPDX-FileCopyrightText: © 2022 ekvip automation GmbH <info@ekvip.de>
| SPDX-License-Identifier: Apache-2.0
| For details check: Apache-2.0_

.. _Apache-2.0: https://www.apache.org/licenses/LICENSE-2.0

.. </legal notes>



*)

METHOD isValid
VAR_INPUT
	(* memory address *)
	address		:PVOID;
	(* number of bytes *)
	sizeInBytes	:UDINT;
	(* message if the assertion is false *)
	message		:AssertMessage;
END_VAR
VAR
	{attribute 'displaymode':='hex'}
	stackAddress	:__XWORD;
	testPointer		:POINTER TO BYTE := NULL;
	testByte		:BYTE;
	exc				: __SYSTEM.ExceptionCode;
END_VAR
VAR CONSTANT
	{attribute 'displaymode':='hex'}
	NULL :__XWORD := 0;
	{attribute 'displaymode':='hex'}
	REGISTER_SIZE :UDINT := SIZEOF(__XWORD);
	(* 0x400000 is 4096kb and this is the max configurable stack size *)
	{attribute 'displaymode':='hex'}
	MAX_STACK_SIZE :__XWORD := 16#0040_0000;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[stackAddress := ADR(stackAddress);
IF (
	(address = NULL) 
	OR_ELSE (sizeInBytes = NULL)
	//check for overflow
	OR_ELSE (address + sizeInBytes < address)
	OR_ELSE (
		(Tc2_System.F_CheckMemoryArea(pData := address, nSize := sizeInBytes) = Tc2_System.E_TcMemoryArea.Unknown)
		AND_THEN (address < ABS(stackAddress - MAX_STACK_SIZE))
	)
) THEN
	THIS^.assertionWasWrong(message, THIS^.getDebugInfo('isValid'));
ELSE
	testPointer := address;
	__TRY
		testByte := testPointer^;
	__CATCH(exc)
		THIS^.assertionWasWrong(message, THIS^.getDebugInfo('isValid'));
	__ENDTRY
END_IF

]]></ST>
      </Implementation>
    </Method>
