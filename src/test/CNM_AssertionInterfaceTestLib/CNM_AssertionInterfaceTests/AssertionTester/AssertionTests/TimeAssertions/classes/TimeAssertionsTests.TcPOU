    <Declaration><![CDATA[(*

short summary
==================
This class tests all methods of TIME assertions
with various combinations of inputs and checks expected 
outputs are satisfied or not

.. <legal notes>

legal notes
=================
| SPDX-FileCopyrightText: © 2022 ekvip automation GmbH <info@ekvip.de>
| SPDX-License-Identifier: Apache-2.0
| For details check: Apache-2.0_

.. _Apache-2.0: https://www.apache.org/licenses/LICENSE-2.0

.. </legal notes>

methods and properties
======================

*)


FUNCTION_BLOCK TimeAssertionsTests EXTENDS AbstractAssertorTest
VAR
	
END_VAR
VAR CONSTANT
	(* prefix to unify the test names *)
	TEST_NAME_PREFIX		:STRING := 'assert.times.simple.';
	(* max value of a DWORD *)
	{attribute 'hide'}
	MAX_DWORD				:DWORD := 16#FFFF_FFFF;
	(* min value of a DWORD *)
	{attribute 'hide'}
	MIN_DWORD				:DWORD := 16#0000_0000;
	(* only the msb is set *)
	{attribute 'hide'}
	MSB_ONLY				:DWORD := 16#8000_0000;
	(* only the msb is set *)
	{attribute 'hide'}
	LSB_ONLY				:DWORD := 16#0000_0001;
	(* all bits set except the msb *)
	{attribute 'hide'}
	WITHOUT_MSB 			:DWORD := 16#7FFF_FFFF;
	(* for every nibble is the msb set only *)
	{attribute 'hide'}
	ALL_NIBBLE_MSB			:DWORD := 16#8888_8888;
	(* for every nibble is the msb unset only *)
	{attribute 'hide'}
	WITHOUT_NIBBLE_MSB		:DWORD := 16#7777_7777;
	(* for every nibble is the lsb set only *)
	{attribute 'hide'}
	ALL_NIBBLE_LSB			:DWORD := 16#1111_1111;
	(* every bit is set except the lsb *)
	{attribute 'hide'}
	LSB_IS_UNSET			:DWORD := 16#FFFF_FFFE;
	(* max vale of time *)
	TIME_MAX				:TIME := TO_TIME(MAX_DWORD);
	(* min vale of time *)
	TIME_MIN				:TIME := TO_TIME(MIN_DWORD);
	(* only the msb is set *)
	TIME_MSB_ONLY			:TIME := TO_TIME(MSB_ONLY);
	(* all bits set except the msb *)
	TIME_WITHOUT_MSB 		:TIME := TO_TIME(WITHOUT_MSB);
	(* for every nibble is the msb set only *)
	TIME_ALL_NIBBLE_MSB		:TIME := TO_TIME(ALL_NIBBLE_MSB);
	(* for every nibble is the msb unset only *)
	TIME_WITHOUT_NIBBLE_MSB	:TIME := TO_TIME(WITHOUT_NIBBLE_MSB);
	(* for every nibble is the lsb set only *)
	TIME_ALL_NIBBLE_LSB		:TIME := TO_TIME(ALL_NIBBLE_LSB);	
	(* every bit is set except the lsb *)
	TIME_LSB_IS_UNSET		:TIME := TO_TIME(LSB_IS_UNSET);
	(* only the msb is set *)
	TIME_LSB_ONLY			:TIME := TO_TIME(LSB_ONLY);
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[THIS^.runTimeHasExpired();
THIS^.runTimeHasNotExpired();
THIS^.runValueIsEqualToTests();
THIS^.runValueIsGreaterThanTests();
THIS^.runValueIsInRangeOfTests();
THIS^.runValueIsLessThanTests();
THIS^.runValueIsMaxTests();
THIS^.runValueIsMinTests();
THIS^.runValueIsNotEqualToTests();

]]></ST>
    </Implementation>
