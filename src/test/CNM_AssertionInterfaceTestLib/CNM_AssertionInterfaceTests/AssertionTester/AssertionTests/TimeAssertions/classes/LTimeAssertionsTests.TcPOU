    <Declaration><![CDATA[(*

short summary
==================
This class tests all methods of LTIME assertions
with various combinations of inputs and checks expected 
outputs are satisfied or not

.. <legal notes>

legal notes
=================
| SPDX-FileCopyrightText: © 2022 ekvip automation GmbH <info@ekvip.de>
| SPDX-License-Identifier: Apache-2.0
| For details check: Apache-2.0_

.. _Apache-2.0: https://www.apache.org/licenses/LICENSE-2.0

.. </legal notes>

methods and properties
======================

*)


FUNCTION_BLOCK LTimeAssertionsTests EXTENDS AbstractAssertorTest
VAR
	
END_VAR
VAR CONSTANT
	(* prefix to unify the test names *)
	TEST_NAME_PREFIX			:STRING := 'assert.times.long.';
	(* max value of a LWORD *)
	{attribute 'hide'}
	MAX_LWORD					:LWORD := 16#FFFF_FFFF_FFFF_FFFF;
	(* min value of a LWORD *)
	{attribute 'hide'}
	MIN_LWORD					:LWORD := 16#0000_0000_0000_0000;
	(* only the msb is set *)
	{attribute 'hide'}
	MSB_ONLY					:LWORD := 16#8000_0000_0000_0000;
	(* only the msb is set *)
	{attribute 'hide'}
	LSB_ONLY					:LWORD := 16#0000_0000_0000_0001;
	(* all bits set except the msb *)
	{attribute 'hide'}
	WITHOUT_MSB 				:LWORD := 16#7FFF_FFFF_FFFF_FFFF;
	(* for every nibble is the msb set only *)
	{attribute 'hide'}
	ALL_NIBBLE_MSB				:LWORD := 16#8888_8888_8888_8888;
	(* for every nibble is the msb unset only *)
	{attribute 'hide'}
	WITHOUT_NIBBLE_MSB			:LWORD := 16#7777_7777_7777_7777;
	(* for every nibble is the lsb set only *)
	{attribute 'hide'}
	ALL_NIBBLE_LSB				:LWORD := 16#1111_1111_1111_1111;
	(* every bit is set except the lsb *)
	{attribute 'hide'}
	LSB_IS_UNSET				:LWORD := 16#FFFF_FFFF_FFFF_FFFE;
	(* max vale of time *)
	LTIME_MAX					:LTIME := TO_LTIME(MAX_LWORD);
	(* min vale of time *)
	LTIME_MIN					:LTIME := TO_LTIME(MIN_LWORD);
	(* only the msb is set *)
	LTIME_MSB_ONLY				:LTIME := TO_LTIME(MSB_ONLY);
	(* all bits set except the msb *)
	LTIME_WITHOUT_MSB 			:LTIME := TO_LTIME(WITHOUT_MSB);
	(* for every nibble is the msb set only *)
	LTIME_ALL_NIBBLE_MSB		:LTIME := TO_LTIME(ALL_NIBBLE_MSB);
	(* for every nibble is the msb unset only *)
	LTIME_WITHOUT_NIBBLE_MSB	:LTIME := TO_LTIME(WITHOUT_NIBBLE_MSB);
	(* for every nibble is the lsb set only *)
	LTIME_ALL_NIBBLE_LSB		:LTIME := TO_LTIME(ALL_NIBBLE_LSB);	
	(* every bit is set except the lsb *)
	LTIME_LSB_IS_UNSET			:LTIME := TO_LTIME(LSB_IS_UNSET);
	(* only the msb is set *)
	LTIME_LSB_ONLY				:LTIME := TO_LTIME(LSB_ONLY);
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[THIS^.runTimeHasExpired();
THIS^.runTimeHasNotExpired();
THIS^.runValueIsEqualToTests();
THIS^.runValueIsGreaterThanTests();
THIS^.runValueIsInRangeOfTests();
THIS^.runValueIsLessThanTests();
THIS^.runValueIsMaxTests();
THIS^.runValueIsMinTests();
THIS^.runValueIsNotEqualToTests();]]></ST>
    </Implementation>
